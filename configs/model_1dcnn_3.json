{
    "features": [
        "LabelEncoding",
        "NumericFeatures",
        "IntensityFeatures",
        "IntensityDiffFeatures"
    ],
    "data_type": {
        "target": "target"
    },
    "cv": {
        "n_splits": 5,
        "shuffle": true,
        "random_state": 71
    },
    "adversarial_validation": {
        "lgb_model_params": {
            "objective": "binary",
            "metric": "auc",
            "boosting": "gbdt",
            "max_depth": 5,
            "num_leaves": 50,
            "learning_rate": 0.1,
            "colsample_bytree": 0.7,
            "subsample": 0.1,
            "subsample_freq": 1,
            "seed": 71,
            "bagging_seed": 71,
            "feature_fraction_seed": 71,
            "drop_seed": 71,
            "verbose": -1
        },
        "lgb_train_params": {
            "num_boost_round": 1000,
            "early_stopping_rounds": 100,
            "verbose_eval": 100
        }
    },
    "model": {
        "name": "1dcnn",
        "model_params": {
            "lr": 1e-4,
            "epochs": 100,
            "batch_size": 64,
            "patience": 20,
            "cyclic_shift__alpha": 0.1
        }
    },
    "dataset": {
        "input_directory": "data/input/",
        "intermediate_directory": "data/interim/",
        "feature_directory": "data/features/",
        "output_directory": "data/output/"
    }
}